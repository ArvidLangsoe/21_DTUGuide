<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/white"
    tools:context="com.arvid.dtuguide.NavigateToDTUActivity">

    <TextView
        android:id="@+id/NavigateDisc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="388dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:shadowColor="@color/cards_and_dialogs_color"
        android:shadowDx="0"
        android:shadowDy="0"
        android:shadowRadius="0"
        android:text="Tryk på det campus du skal finde vej til!"
        android:textColor="@android:color/black"
        android:textColorLink="@color/colorPrimary"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView6"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/red_circle_lyngby"
        android:layout_width="130dp"
        android:layout_height="118dp"
        app:layout_constraintBottom_toBottomOf="@+id/image_lyngby"
        app:layout_constraintEnd_toEndOf="@+id/image_lyngby"
        app:layout_constraintStart_toStartOf="@+id/image_lyngby"
        app:layout_constraintTop_toTopOf="@+id/image_lyngby"
        app:srcCompat="@drawable/red_circle" />

    <ImageView
        android:id="@+id/red_circle_ballerup"
        android:layout_width="130dp"
        android:layout_height="118dp"
        android:layout_marginEnd="1dp"
        app:layout_constraintBottom_toBottomOf="@+id/image_ballerup
"
        app:layout_constraintEnd_toEndOf="@+id/image_ballerup
"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/image_ballerup
"
        app:layout_constraintTop_toTopOf="@+id/image_ballerup
"
        app:srcCompat="@drawable/red_circle" />

    <ImageView
        android:id="@+id/red_circle_risoo"
        android:layout_width="130dp"
        android:layout_height="118dp"
        app:layout_constraintBottom_toTopOf="@+id/text_risoo"
        app:layout_constraintEnd_toEndOf="@+id/image_risoo"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/image_risoo"
        app:layout_constraintTop_toTopOf="@+id/image_risoo"
        app:srcCompat="@drawable/red_circle" />

    <ImageView
        android:id="@+id/image_ballerup
"
        android:layout_width="131dp"
        android:layout_height="114dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.966"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.407"
        app:srcCompat="@drawable/ballerup_icon" />

    <ImageView
        android:id="@+id/image_lyngby"
        android:layout_width="132dp"
        android:layout_height="114dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:elevation="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.033"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.407"
        app:srcCompat="@drawable/lyngby_icon" />

    <ImageView
        android:id="@+id/image_risoo"
        android:layout_width="130dp"
        android:layout_height="114dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:elevation="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.691"
        app:srcCompat="@drawable/risoo_icon" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="106dp"
        android:layout_height="137dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/dtu_logo" />

    <TextView
        android:id="@+id/text_lyngby"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:text="Lyngby"
        app:layout_constraintBottom_toTopOf="@+id/image_lyngby"
        app:layout_constraintEnd_toEndOf="@+id/image_lyngby"
        app:layout_constraintHorizontal_bias="0.486"
        app:layout_constraintStart_toStartOf="@+id/image_lyngby"
        app:layout_constraintTop_toBottomOf="@+id/image_lyngby"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/text_risoo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:text="Risø"
        app:layout_constraintBottom_toTopOf="@+id/image_risoo"
        app:layout_constraintEnd_toEndOf="@+id/image_risoo"
        app:layout_constraintHorizontal_bias="0.505"
        app:layout_constraintStart_toStartOf="@+id/image_risoo"
        app:layout_constraintTop_toBottomOf="@+id/image_risoo"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/text_ballerup"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="Ballerup"
        app:layout_constraintBottom_toTopOf="@+id/image_ballerup
"
        app:layout_constraintEnd_toEndOf="@+id/red_circle_ballerup"
        app:layout_constraintHorizontal_bias="0.534"
        app:layout_constraintStart_toStartOf="@+id/red_circle_ballerup"
        app:layout_constraintTop_toBottomOf="@+id/image_ballerup
"
        app:layout_constraintVertical_bias="0.056" />

</android.support.constraint.ConstraintLayout>
